{
  "name": "go-templ-gotodefinition-redirect",
  "displayName": "go-templ-goToDefinition-redirect",
  "description": "Hijacks go to definition to go from autogenerated _templ.go files to .templ files",
  "author": {
    "name": "Razvan Rusu",
    "email": "razvan.rusu19@imperial.ac.uk"
  },
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.goToDefinitionRedirect",
        "title": "Go to Definition with Redirect"
      }
    ],
    "keybindings": [
      {
        "command": "extension.goToDefinitionRedirect",
        "key": "cmd+shift+D",
        "when": "editorTextFocus && editorLangId == go"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}
